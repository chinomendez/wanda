{
  "name": "@wonderflow/icons",
  "description": "Wanda's iconography",
  "version": "1.0.0",
  "author": "Wonderflow",
  "license": "MIT",
  "main": "dist/index",
  "exports": {
    "./sprite": "./dist/sprite.svg",
    "./structure": "./dist/structure.json"
  },
  "typesVersions": {
    "*": {
      "sprite": [
        "dist/sprite.d.ts"
      ],
      "structure": [
        "dist/structure.json"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=8.x"
  },
  "scripts": {
    "build": "run-s prepare build:copy build:optimize build:sprite build:clean",
    "prepare": "rimraf dist",
    "build:copy": "copyfiles \"svgs/**/*.svg\" dist/",
    "build:optimize": "svgo ./dist/svgs/**/* --config=svgo.config.js --quiet",
    "build:sprite": "node ./sprites.js",
    "build:clean": "rimraf dist/svgs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/wanda.git"
  },
  "keywords": [
    "design-system",
    "wonderflow",
    "components",
    "react",
    "icons"
  ],
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.8",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "babel-loader": "8.2.3",
    "copyfiles": "2.4.1",
    "css-loader": "6.5.1",
    "directory-tree": "3.0.1",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "4.5.2",
    "icons": "1.0.0",
    "nanospinner": "0.6.0",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "postcss-loader": "6.2.1",
    "react-components": "0.5.1",
    "react-cosmos": "5.6.6",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "svgo": "2.8.0",
    "svgstore": "3.0.1"
  }
}
